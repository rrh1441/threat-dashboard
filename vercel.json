{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/py-api/stix_poc",
        "dest": "/api/stix_poc.py"
      }
    ]
  }