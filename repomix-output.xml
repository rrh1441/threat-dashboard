    const finalCsv = csvLines.join("\n");

    return new Response(finalCsv, {
      status: 200,
      headers: {
        "Content-Type": "text/csv",
        "Content-Disposition": 'attachment; filename="yearly_counts.csv"',
      },
    });
  } catch (error) {
